<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFramework>net6.0-windows10.0.22621.0</TargetFramework>
		<Nullable>enable</Nullable>
		<UseWPF>true</UseWPF>
		<StartupObject>WPFAsyncAwait.App</StartupObject>
		<ApplicationIcon>wrs_logo.ico</ApplicationIcon>
		<PlatformTarget>x64</PlatformTarget>
		<PackageIcon>b_trans.png</PackageIcon>
		<Platforms>AnyCPU;x64</Platforms>
		<PublishSingleFile>true</PublishSingleFile>
	</PropertyGroup>

	<ItemGroup>
		<Content Include="wrs_logo.ico" />
	</ItemGroup>

	<ItemGroup>
		<None Update="b_trans.png">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

</Project>
